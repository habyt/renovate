{
  "extends": ["config:base"],
  "labels": ["patch"],
  "dependencyDashboard": true,
  "stabilityDays": 3,
  "prCreation": "not-pending",
  "prHourlyLimit": 2,
  "packageRules": [
    {
      "updateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "groupName": "equ",
      "packagePatterns": ["^@equ-lang/"],
      "automerge": true
    },
    {
      "groupName": "homefully",
      "packagePatterns": ["^@homefully/"],
      "automerge": true

    },
    {
      "groupName": "definitelyTyped",
      "packagePatterns": ["^@types/"],
      "automerge": true

    },
    {
      "groupName": "jest",
      "packageNames": ["jest", "ts-jest", "@types/jest"],
      "automerge": true
    }
  ]
}
